{
  "name": "chatiz",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "parcel src/index.html --no-cache --port 1234",
    "server": "node ./server.js",
    "build": "parcel build src/index.html",
    "build:scss-types": "tsm src --nameFormat none --exportType default",
    "watch": "parcel watch src/index.html",
    "lint": "eslint . --ext .ts",
    "slint": "stylelint \"**/*.scss\"",
    "check": "tsc --noEmit",
    "start": "npm run build && npm run server"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "author": "OLEG XOPEB",
  "license": "ISC",
  "targets": {
    "global": {
      "context": "browser",
      "outputFormat": "global"
    }
  },
  "peerDependencies": {
    "typescript": "*"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@parcel/transformer-sass": "2.7.0",
    "@parcel/validator-typescript": "2.7.0",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.42.1",
    "eslint": "^8.26.0",
    "eslint-config-airbnb": "19.0.4",
    "express": "4.18.2",
    "parcel": "2.7.0",
    "parcel-transformer-hbs": "1.0.4",
    "sass": "1.55.0",
    "stylelint": "14.14.1",
    "stylelint-config-standard-scss": "6.1.0",
    "stylelint-scss": "4.3.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "handlebars": "4.7.7",
    "uuid": "9.0.0"
  }
}
